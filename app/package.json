{
  "name": "@qi/app",
  "version": "0.3.0",
  "description": "Qi V2 Agent - CLI + Agent with Command, Prompt & Context Continuation",
  "type": "module",
  "scripts": {
    "qi-code": "bun run src/qi-code.ts",
    "qi-code:test": "bun run src/test-qi-code.ts",
    "demo:three-type": "bun run src/demos/three-type-demo.ts",
    "demo:classifier": "bun run src/demos/classifier-module-demo.ts",
    "demo:simple-classifier": "bun run src/demos/simple-classifier-demo.ts",
    "demo:classifier-cli": "bun run src/demos/classifier-cli-demo.ts",
    "demo:classifier-cli-test": "bun run src/demos/classifier-cli-test.ts",
    "demo:simple-prompt": "bun run src/demos/simple-prompt-demo.ts",
    "demo:multi-llm": "bun run src/demos/multi-llm-demo.ts",
    "demo:config-manager": "bun run src/demos/config-manager-demo.ts",
    "demo:prompt-manager": "bun run src/demos/prompt-manager-demo.ts",
    "demo:cli-ink": "bun run src/demos/cli-ink-demo.ts",
    "demo:cli-neo-blessed": "bun run src/demos/cli-neo-blessed-demo.ts",
    "demo:cli-complete": "bun run src/demos/cli-complete-test.ts",
    "demo:advanced-classifier": "bun run src/demos/advanced-classifier-demo.ts",
    "demo:cli-advanced-integration": "bun run src/demos/cli-advanced-parser-integration.ts",
    "demo:classifier-fix-test": "bun run src/demos/classifier-fix-test.ts",
    "demo:workflow-integration": "bun run src/demos/workflow-integration-demo.ts",
    "demo:workflow-extractor": "bun run src/demos/workflow-extractor-test.ts",
    "demo:context-manager": "bun run src/demos/context/context-manager-demo.ts",
    "demo:context-continuation": "bun run src/demos/context/context-continuation-demo.ts",
    "test:context": "bun run src/demos/context/context-test.ts",
    "test:langchain": "bun run src/demos/prompts/langchain-prompt-test.ts",
    "finetune:setup": "python src/fine-tuning/setup_uv.py",
    "finetune:generate-data": "cd src/fine-tuning && uv run generate_data.py",
    "finetune:train": "cd src/fine-tuning && uv run train.py",
    "finetune:sync": "cd src/fine-tuning && uv sync",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --watch",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "check": "bun run typecheck && bun run lint && bun run test"
  },
  "dependencies": {
    "@inkjs/ui": "^2.0.0",
    "@langchain/core": "^0.3.66",
    "@xstate/react": "^6.0.0",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "ink": "^6.1.0",
    "multi-llm-ts": "^4.2.2",
    "neo-blessed": "^0.2.0",
    "react": "^19.0.0",
    "xstate": "^5.20.1",
    "yaml": "^2.8.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "vitest": "^3.0.0",
    "@types/react": "^19.0.0",
    "@types/blessed": "^0.1.25",
    "bun-types": "^1.1.38",
    "typescript": "^5.8.3"
  }
}
