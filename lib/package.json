    "@langchain/google-genai": "^0.1.3",
    "@langchain/groq": "^0.1.3",
    "@langchain/langgraph": "^0.4.0",
    "@langchain/ollama": "^0.2.3",
    "@langchain/openai": "^0.3.15",
    "@modelcontextprotocol/sdk": "^1.16.0",
    "@qi": "file:../../qi-v2-qicore/typescript",
    "chalk": "^5.5.0",
    "ink": "^6.1.0",
