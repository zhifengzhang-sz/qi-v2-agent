    "./config": "./dist/config/index.js",
    "./utils": "./dist/utils/index.js",
    "./context": "./dist/context/index.js",
    "./classifier": "./dist/classifier/index.js",
    "./classifier/shared/provider-map": "./dist/classifier/shared/provider-map.js",
