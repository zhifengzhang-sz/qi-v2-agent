{
  "name": "@qi/agent",
  "version": "v-0.1.0-alpha.1",
  "description": "Agent capabilities with AutoGen/AgentChat abstractions, context engineering, and tool management",
  "private": false,
  "workspaces": [
    "lib"
  ],
  "main": "lib/dist/index.js",
  "types": "lib/dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/dist/index.d.ts",
      "default": "./lib/dist/index.js"
    },
    "./context-engineering": {
      "types": "./lib/dist/context-engineering/index.d.ts",
      "default": "./lib/dist/context-engineering/index.js"
    },
    "./workflow-engine": {
      "types": "./lib/dist/workflow-engine/index.d.ts",
      "default": "./lib/dist/workflow-engine/index.js"
    },
    "./sub-agent": {
      "types": "./lib/dist/sub-agent/index.d.ts",
      "default": "./lib/dist/sub-agent/index.js"
    },
    "./tools": {
      "types": "./lib/dist/tools/index.d.ts",
      "default": "./lib/dist/tools/index.js"
    }
  },
  "scripts": {
    "build": "bun run --cwd lib build",
    "test": "bun run --cwd lib test",
    "test:watch": "bun run --cwd lib test --watch",
    "typecheck": "bun run --cwd lib type-check",
    "lint": "bun run --cwd lib lint",
    "format": "bun run --cwd lib format",
    "clean": "rm -rf lib/dist",
    "check": "bun run typecheck && bun run lint && bun run test"
  },
  "peerDependencies": {
    "@qi/base": "*",
    "@qi/core": "*",
    "@qi/amsg": "*",
    "@qi/cli": "*"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "bun-types": "^1.1.38",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4"
  },
  "optionalDependencies": {
    "autogen-agentchat": "*",
    "autogen": "*"
  }
}
